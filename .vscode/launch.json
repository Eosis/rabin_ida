  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'rabin_ida'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=rabin_ida"
        ],
        "filter": {
          "name": "rabin_ida",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug benchmark 'ida_benchmark'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bench=ida_benchmark",
          "--package=rabin_ida"
        ],
        "filter": {
          "name": "ida_benchmark",
          "kind": "bench"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}